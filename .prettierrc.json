{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "importOrder": [
    "^[./]*lib(.*)$",
    "^[./]*__mocks__(.*)$",
    "^[./]*types(.*)$",
    "^[./]*config(.*)$",
    "^[./]*context(.*)$",
    "^[./]*hooks(.*)$",
    "^[./]*utils(.*)$",
    "^[./]*icons(.*)$",
    "^[./]*components(.*)$",
    "^[./]*compositions(.*)$",
    "^[./]*docs(.*)$",
    "^[./]",
    "^[./]*styles(.*)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList": ["jsx", "typescript"]
}
