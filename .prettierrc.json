{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "importOrder": [
    "^[./]*lib(.*)$",
    "^[./]*__mocks__(.*)$",
    "^(../)+types(.*)$",
    "^types(.*)$",
    "^[./]*config(.*)$",
    "^[./]*context(.*)$",
    "^[./]*hooks(.*)$",
    "^(../)+utils(.*)$",
    "^utils(.*)$",
    "^[./]*icons(.*)$",
    "^[./]*components(.*)$",
    "^[./]*compositions(.*)$",
    "^[./]*docs(.*)$",
    "^[../]",
    "^./types(.*)$",
    "^./utils(.*)$",
    "^./[A-Z]+[A-Za-z]+$",
    "^./[a-z]+[A-Za-z]+$",
    "^[./]",
    "^[./]*styles(.*)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList": ["jsx", "typescript"]
}
